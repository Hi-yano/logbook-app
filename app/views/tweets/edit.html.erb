<%= render "shared/mypage" %>
<div class="contents-row">
  <h3 class="tweet-title">編集する</h3>
  <div class="container">
      <%= form_with(model: @tweet, local: true) do |f| %>
        <%= f.file_field :image, id:"log-image" %>
      <div class="tweets-area">
        <%= f.text_area :text, placeholder: "ポイントの感想など", class: "text-form", rows: "10" %>
        </div>
        <%= f.submit "SEND", class:"submit-send-btn" %>
      <% end %>
  </div>
</div>